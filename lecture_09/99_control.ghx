                  <items count="13">
                    <item name="CodeInput" type_name="gh_string" type_code="10">from compas.geometry import Translation
from compas.geometry import Scale
from compas_fab.ghpython.components import coerce_frame
from compas_fab.ghpython.components import create_id

from scriptcontext import sticky as st
import compas_rrc as rrc

key = create_id(ghenv.Component, 'configs')

i = n - 1
element = assembly.element(i)
element_frame = element.frame.transformed(T)

height = 0.012
safety_offset = 0.001
place_frame = element_frame.transformed(Translation.from_vector([0, 0, (height / 2.) + safety_offset]))

approach_offset = 0.060  # 60mm
t_approach = Translation.from_vector([0, 0, approach_offset])
pick_frame = pick_tcf
approach_pick_frame = pick_frame.transformed(t_approach)
approach_place_frame = place_frame.transformed(t_approach)

if check:
    planes = [approach_pick_frame, pick_frame, approach_place_frame, place_frame]
    planes_at_t0cf = robot.from_tcf_to_t0cf(planes)
    configs = []
    for frame in planes_at_t0cf:
        configs.append(robot.inverse_kinematics(frame))
    
    st[key] = configs

configs = st.get(key)

#a = []
#a.append(rg.Plane(rg.Point3d(*list(pick_frame.point)), rg.Vector3d(*list(pick_frame.xaxis)), rg.Vector3d(*list(pick_frame.yaxis))))
#a.append(rg.Plane(rg.Point3d(*list(approach_pick_frame.point)), rg.Vector3d(*list(approach_pick_frame.xaxis)), rg.Vector3d(*list(approach_pick_frame.yaxis))))
#a.append(rg.Plane(rg.Point3d(*list(approach_place_frame.point)), rg.Vector3d(*list(approach_place_frame.xaxis)), rg.Vector3d(*list(approach_place_frame.yaxis))))
#a.append(rg.Plane(rg.Point3d(*list(place_frame.point)), rg.Vector3d(*list(place_frame.xaxis)), rg.Vector3d(*list(place_frame.yaxis))))

print ('Pick at {}, {}, {}'.format(*list(pick_frame.point)))
print ('Place at {}, {}, {}'.format(*list(place_frame.point)))


# Convert to RRC
S = Scale.from_factors([1000, 1000, 1000])
pick_frame = pick_frame.transformed(S)
place_frame = place_frame.transformed(S)
approach_pick_frame = approach_pick_frame.transformed(S)
approach_place_frame = approach_place_frame.transformed(S)


if execute:
    # Create ABB Client
    abb = rrc.AbbClient(ros_client, '/rob1')

    # Reset signals
    abb.send(rrc.SetDigital('doUnitC106Out2',0))

    # Set tool
    abb.send(rrc.SetTool('t_RRC_Vacuum_Gripper'))

    # Set work object
    abb.send(rrc.SetWorkObject('wobj0'))

    # Define speed
    speed = 100

    # Move over pick position
    abb.send(rrc.MoveToFrame(approach_pick_frame, speed, rrc.Zone.Z10))

    # Move to pick position
    abb.send(rrc.MoveToFrame(pick_frame, speed, rrc.Zone.FINE))

    # Vacuum on
    abb.send(rrc.SetDigital('doUnitC106Out2',1))

    # Move over pick position
    abb.send(rrc.MoveToFrame(approach_pick_frame, speed, rrc.Zone.Z10))

    # Move over place position
    abb.send(rrc.MoveToFrame(approach_place_frame, speed, rrc.Zone.Z10))

    # Move to place position
    abb.send_and_wait(rrc.MoveToFrame(place_frame, speed, rrc.Zone.FINE))

    # Vacuum off
    abb.send_and_wait(rrc.SetDigital('doUnitC106Out2',0))

    # Move over place position
    abb.send(rrc.MoveToFrame(approach_place_frame, speed, rrc.Zone.Z10))
