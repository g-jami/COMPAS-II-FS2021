from compas_ghpython.artists import MeshArtist
#from helpers import traversal_buildup_sequence
#from helpers import traversal_breadth_first_ordering
from helpers import traversal_linearly_ordered
from compas_ghpython.artists import FrameArtist

elements = []
frames = []

for j, n in enumerate(traversal_linearly_ordered(assembly)):
    geom = assembly.element(n).geometry_at_placement
    geom = geom.transformed(T)
    mesh = MeshArtist(geom).draw()
    elements.append(mesh)

    fa = FrameArtist(assembly.element(n).frame.transformed(T), scale=.02)
    a, b = fa.draw()
    frames.extend(a)
    frames.extend(b)
